        <section id="doge-scrolly">

        <!-- define the scroll sections -->
        <% const blocks=
            [
            { 
                "filename" : "gas" , 
                "anno" : "_blank-slide_" 
            }, 
            { 
                "filename" : "gas_1" , 
                "anno": "The Dogefather"
            },
            { 
                "filename" : "gas_2" , 
                "anno": "<div class='h5 fw-bold'> Doge Title </div> Much Doge Such Wow" 
